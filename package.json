{
    "name": "@cere/media-sdk-js",
    "description": "Cere Media SDK for uploading and retrieving encrypted, token backed content.",
    "version": "1.0.7",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Cerebellum-Network/global-nft-registry-sdk-js"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "browser": "./dist/browser.js",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./browser": {
            "require": "./dist/browser.cjs",
            "import": "./dist/browser.js",
            "default": "./dist/browser.js"
        }
    },
    "typesVersions": {
        "*": {
            "browser": [
                "index"
            ]
        }
    },
    "author": "Cere Network <contact@cere.io (https://www.cere.io/)",
    "license": "Apache-2.0",
    "scripts": {
        "build:cjs": "TARGET=build:cjs NODE_ENV=production webpack",
        "build:mjs": "TARGET=build:mjs NODE_ENV=production webpack",
        "build:browser": "BROWSERSLIST_ENV=browser npm run build:cjs && BROWSERSLIST_ENV=browser npm run build:mjs",
        "build:node": "BROWSERSLIST_ENV=node npm run build:cjs && BROWSERSLIST_ENV=node npm run build:mjs",
        "build": "npm run clean && npm run build:browser && npm run build:node && npm run types ",
        "types": "tsc -p tsconfig.json --emitDeclarationOnly",
        "clean": "rimraf dist package",
        "test": "jest --passWithNoTests --silent",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-typescript": "^7.18.6",
        "@types/detect-port": "^1.3.2",
        "@types/jest": "^27.4.0",
        "@types/lodash": "^4.14.182",
        "@types/node": "^16.0.0",
        "babel-loader": "^8.2.5",
        "clean-publish": "^4.2.0",
        "crypto-browserify": "^3.12.0",
        "docker-compose": "^0.23.17",
        "eslint-plugin-jest": "^27.4.0",
        "jest": "^29.7.0",
        "lodash": "^4.17.21",
        "prettier": "^2.5.1",
        "rimraf": "^5.0.5",
        "stream-browserify": "^3.0.0",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.5.5",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "tsconfig-paths": "^4.2.0"
    }
}
